      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "text", "value": null }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 3, "w": 4, "x": 12, "y": 1 },
      "id": 4,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "sum(stoker_controller_gateways_discovered{namespace=~\"$namespace\"}) or vector(0)",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Total Gateways",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
