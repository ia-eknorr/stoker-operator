      },
      "targets": [
        { "expr": "histogram_quantile(0.50, sum by (le) (rate(stoker_controller_reconcile_duration_seconds_bucket{namespace=~\"$namespace\"}[$__rate_interval])))", "legendFormat": "p50", "refId": "A" },
        { "expr": "histogram_quantile(0.95, sum by (le) (rate(stoker_controller_reconcile_duration_seconds_bucket{namespace=~\"$namespace\"}[$__rate_interval])))", "legendFormat": "p95", "refId": "B" },
