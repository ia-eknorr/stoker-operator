      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "mappings": [],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "green", "value": null }, { "color": "orange", "value": 1 }, { "color": "red", "value": 3 }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 3, "w": 4, "x": 20, "y": 1 },
      "id": 6,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "sum(stoker_controller_gateways_missing_sidecar{namespace=~\"$namespace\"}) or vector(0)",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Missing Sidecars",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "links": [
            {
              "title": "View Details",
              "url": "/d/stoker-detail?var-datasource=${datasource}&var-namespace=${__field.labels.namespace}&var-cr=${__field.labels.name}"
            }
          ],
          "mappings": [
            { "options": { "0": { "text": "Not Ready" } }, "type": "value" },
            { "options": { "1": { "text": "Ready" } }, "type": "value" }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": 0 }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      },
      "gridPos": { "h": 7, "w": 12, "x": 0, "y": 4 },
      "id": 7,
      "options": {
        "colorMode": "background",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "percentChangeColorMode": "standard",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "auto",
        "wideLayout": true
      },
      "pluginVersion": "12.4.0",
      "targets": [
        {
          "expr": "max by (name, namespace) (stoker_controller_cr_ready{namespace=~\"$namespace\"})",
          "legendFormat": "{{ name }}",
          "refId": "A"
        }
      ],
      "title": "CR Ready",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
      "fieldConfig": {
        "defaults": {
          "color": { "mode": "thresholds" },
          "links": [
            {
              "title": "View Details",
              "url": "/d/stoker-detail?var-datasource=${datasource}&var-namespace=${__field.labels.namespace}&var-cr=${__field.labels.name}"
            }
          ],
          "mappings": [
            {
              "options": {
                "0": { "color": "text", "text": "0/0" },
                "1": { "color": "red", "text": "0/1" },
                "2": { "color": "red", "text": "0/2" },
                "3": { "color": "red", "text": "0/3" },
                "4": { "color": "red", "text": "0/4" },
                "5": { "color": "red", "text": "0/5" },
                "101": { "color": "green", "text": "1/1" },
                "102": { "color": "red", "text": "1/2" },
                "103": { "color": "red", "text": "1/3" },
                "104": { "color": "red", "text": "1/4" },
                "105": { "color": "red", "text": "1/5" },
                "202": { "color": "green", "text": "2/2" },
                "203": { "color": "orange", "text": "2/3" },
                "204": { "color": "red", "text": "2/4" },
                "205": { "color": "red", "text": "2/5" },
                "303": { "color": "green", "text": "3/3" },
                "304": { "color": "orange", "text": "3/4" },
                "305": { "color": "red", "text": "3/5" },
                "404": { "color": "green", "text": "4/4" },
                "405": { "color": "orange", "text": "4/5" },
                "505": { "color": "green", "text": "5/5" }
              },
              "type": "value"
            }
          ],
          "thresholds": { "mode": "absolute", "steps": [{ "color": "red", "value": 0 }, { "color": "green", "value": 1 }] }
        },
        "overrides": []
      },
