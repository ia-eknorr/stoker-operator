      "id": 5,
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "justifyMode": "auto",
        "orientation": "auto",
        "reduceOptions": { "calcs": ["lastNotNull"], "fields": "", "values": false },
        "showPercentChange": false,
        "textMode": "value"
      },
      "targets": [
        {
          "expr": "sum(stoker_controller_gateways_synced{namespace=~\"$namespace\"}) or vector(0)",
          "legendFormat": "",
          "refId": "A"
        }
      ],
      "title": "Synced Gateways",
      "type": "stat"
    },
    {
      "datasource": { "type": "prometheus", "uid": "${datasource}" },
